            parameter="test:2"
        />

	<!--  support for Audio Test Case -->
		<!-- headset detect -->
		<module name="KEY_HEADSET"
				lib_name="nubia_mmi_headset.so"
				enable="1"
                automation="0"
                display_name="headset_key"
                layout="layout_headset_key.xml"
                parameter="keys:headphone,microphone,hangup;"/>

		<!-- Loopback for headset -->
		<module name="AUDIO_HEADSET_LOOPBACK"
				lib_name="nubia_mmi_audio.so"
				enable="1"
                automation="0"
                display_name="audio_headset_loopback"
                layout="layout_headset.xml"
                parameter="type:loopback;tc:225;volume:80;duration:30;file:/data/ftm_pcm_record.wav;"/>

		<module name="AUDIO_PRIMARY_MIC_RECORD"
				lib_name="nubia_mmi_audio.so"
				enable="1"
                automation="0"
                display_name="audio_primary_mic"
                layout="layout_primary_mic.xml"
                parameter="type:loopback;tc:256;volume:80;duration:30;file:/persist/FTM_AP/ftm_pcm_record.wav;"/>

		<module name="AUDIO_NOISE_MIC_RECORD"
				lib_name="nubia_mmi_audio.so"
				enable="1"
                automation="0"
                display_name="audio_noise_mic"
                layout="layout_primary_mic.xml"
                parameter="type:loopback;tc:257;volume:80;duration:30;file:/persist/FTM_AP/ftm_pcm_record.wav;"/>

		<module name="AUDIO_TOP_MIC_RECORD"
				lib_name="nubia_mmi_audio.so"
				enable="1"
                automation="0"
                display_name="audio_top_mic"
                layout="layout_primary_mic.xml"
                parameter="type:loopback;tc:258;volume:80;duration:30;file:/persist/FTM_AP/ftm_pcm_record.wav;"/>

		<!-- Play tone in Handset -->
		<module name="AUDIO_HANDSET_PLAYTONE"
				lib_name="nubia_mmi_audio.so"
				enable="1"
                automation="0"
                display_name="audio_handset_play"
                layout="layout_handset.xml"
                parameter="type:play;tc:5;volume:60;duration:8;file:/system/vendor/etc/nubia_mmi/qualsound.wav;"/>

		<!-- Play tone in Speaker -->
		<module name="AUDIO_SPEAKER_PLAYTONE"
				lib_name="nubia_mmi_audio.so"
				enable="1"
                automation="0"
                display_name="audio_loudspeaker_play"
                layout="layout_loudspeaker.xml"
                parameter="type:play;tc:1;volume:60;duration:8;file:/system/vendor/etc/nubia_mmi/qualsound.wav;"/>

		<!-- SmartPA Calibration -->
		<module name="AUDIO_SMARTPA_CALIBRATION"
			lib_name="nubia_mmi_audio_smartpa_cal.so"
			enable="1"
			automation="0"
			display_name="audio_smartpa_cal"
			layout="layout_smartpa_cal.xml"/>

                <!-- support for NFC Test Case -->
                <module name="NFC"
                        lib_name="nubia_mmi_nfc.so"
                        enable="1"
                        automation="0"
                        display_name="nfc"
                        layout="layout_nfc.xml"
                />

          <module name="BATTERY"
                lib_name="nubia_mmi_battery.so"
                enable="1"
                automation="0"
                display_name="battery"
                layout="layout_battery.xml"
                parameter="method:direct"
                />

        <module name="OTG"
                lib_name="nubia_mmi_otg.so"
                enable="1"
                automation="0"
                display_name="otg"
                layout="layout_otg.xml"
                parameter="type:otg;"/>

    </modules>

    <!-- default specified config for PCBA mode,will over write the default value -->
    <modules_pcba>
    <!-- support key list,please refer to layout_key.xml. -->
        <module name="KEY"
                lib_name="nubia_mmi_key.so"
