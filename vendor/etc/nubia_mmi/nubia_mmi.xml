		<module name="MSENSOR"
                lib_name="nubia_mmi_sensor.so"
                enable="1"
                automation="0"
                display_name="msensor"
                layout="layout_msensor.xml"
                parameter="type:rotation_vector;delay:100;limit=x_min_limit:-1000;x_max_limit:1000;y_min_limit:-1000;y_max_limit:1000;z_min_limit:-1000;z_max_limit:1000;"/>
		<module name="HSENSOR"
                lib_name="nubia_mmi_sensor.so"
                enable="1"
                automation="0"
                display_name="hsensor"
                layout="layout_hsensor.xml"
                parameter="type:hall;delay:100;limit=x_min_limit:-1000;x_max_limit:1000;y_min_limit:-1000;y_max_limit:1000;z_min_limit:-1000;z_max_limit:1000;"/>
		<module name="ACC_CAL"
